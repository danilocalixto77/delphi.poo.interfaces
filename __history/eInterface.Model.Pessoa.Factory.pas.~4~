  eInterface.Model.Interfaces;

Type 
  TModelPessoaFactory = class(TInterfacedObject, iPessoaFactory)
  private

  public
    constructor Create; 
    destructor Destroy; override;
    class function New : iPessoaFactory;
  end;

  implementation
  
  { TModelPessoaFactory }

constructor TModelPessoaFactory.Create;
begin

end;

